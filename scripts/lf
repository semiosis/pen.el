#!/bin/bash
export TTY

( hs "$(basename "$0")" "$@" "#" "<==" "$(ps -o comm= $PPID)" 0</dev/null ) &>/dev/null

# github author/repo may need adjustment
test -f "${GOPATH%%:*}/bin/up" || env CGO_ENABLED=0 go install -ldflags="-s -w" github.com/gokcehan/lf@latest &>/dev/null

${GOPATH%%:*}/bin/lf "$@"
