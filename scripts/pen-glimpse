#!/bin/bash
export TTY

( hs "$(basename "$0")" "$@" "#" "<==" "$(ps -o comm= $PPID)" 0</dev/null ) &>/dev/null

is_tty() {
    # If stout is a tty
    [[ -t 1 ]]
}

# | sed "s=$MIRRORD\(.*\).txt$=\1="

blacklist() {
    grep -v /programs/ci/ |
        grep -v clipboard.txt |
        grep -v /ns/ |
        grep -v /.atom/
}


alt -s glimpse -n "$@" | blacklist | sed "s=^$MIRRORD\\(.*\\)\\.txt:=\\1:=" | {
    if is_tty; then
        ead -j || pak
    else
        cat
    fi
}
