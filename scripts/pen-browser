#!/bin/bash
export TTY

( hs "$(basename "$0")" "$@" "#" "<==" "$(ps -o comm= $PPID)" 0</dev/null ) &>/dev/null

: "${BROWSER:="sps-lg"}"

if test "$BROWSER" = pen-browser || test "$BROWSER" = browser; then
    export BROWSER=sps-lg
fi

# If handled with something else, abort browser
pen-handle-url "$@" && exit "$?"

"$BROWSER" "$@"
