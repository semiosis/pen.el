#!/bin/bash

# Replace:
# 
# [22;24;25;27;28;39;48;5;236m:
# with:
# [22;24;25;27;28;38;5;60;49m

# [22;24;25;27;28;39;49m
# [22;24;25;27;28;38;5;60;49m

vimcat-basic -c "call GeneralSyntax()" "$@" |
    sed 's=\[22;24;25;27;28;39;48;5;236m=[22;24;25;27;28;38;5;60;49;48;5;236m=g' |
    sed 's=\[22;24;25;27;28;39;49m=[22;24;25;27;28;38;5;60;49m=g' |
    pa -E "less -rS"
#