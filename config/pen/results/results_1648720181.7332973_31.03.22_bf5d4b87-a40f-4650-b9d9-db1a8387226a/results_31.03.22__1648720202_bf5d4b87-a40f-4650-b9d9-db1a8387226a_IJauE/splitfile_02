class TestDocGen(unittest.TestCase):
    def test_render_empty(self):
        g = DocGen()
        self.assertEqual(g.render(), '')

    def test_render_numbers_only(self):
        g = DocGen()
        g.numbers('1', '2', '3')
        self.assertEqual(g.render(), numbers_only)
