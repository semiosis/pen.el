    def download_data(self):
        self.url = "https://swcarpentry.github.io/data-201-resources/workshop/data.zip"
        # Download file
        with urllib.request.urlopen(self.url) as src:
            with open(self.zip_file, "wb") as output:
                output.write(src.read())

