
  (char-to-string (+ ?a (1- char))))

(defun pen-modeline-name-letter ()
  (let ((dn (pen-daemon-name)))
    (cond ((string-equal "DEFAULT" dn)
           "ðŸ–Š")
          ((s-matches? "pen-" dn)
           (number-to-letter
            (string-to-number
             (s-replace-regexp
              "pen-emacsd-" ""
              dn))))
          (t
           "e"))))
