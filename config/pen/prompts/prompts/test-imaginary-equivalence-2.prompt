  <delim>

engine: "OpenAI Codex"
temperature: 0.3
max-generated-tokens: 100
top-p: 1.0
stop-sequences:
- "<delim>"
cache: on
vars:
- "A"
- "B"
examples:
- "Semerkhet, the Egyptian king who ruled during the First Dynasty"
- "Moses also known as Moshe Rabbenu"
filter: on
completion: off
insertion: off
n-collate: 3
n-completions: 10
no-uniq-results: on
results-analyser: pen-analyse-true-or-false
postprocessor: sed 's/^\([a-zA-Z]*\).*/\1/'