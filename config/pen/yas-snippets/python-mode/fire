import fire

# python3 solution_01_02_01.py fun

# Import the English language class
from spacy.lang.en import English

def fun():
    # Create the nlp object
    nlp = English()

    # Process a text
    doc = nlp("This is a sentence.")

    # Print the document text
    print(doc.text)


if __name__ == '__main__':
    fire.Fire()