" endif
fun! QuitNvim(...)
    q!
endf
fun! TermAndQuit(...)
    let s:cmd = get(a:, 1, "TERM=vt100 tmux attach -t localhost")
    let s:callbacks = {
                \ 'on_exit': function('QuitNvim')
                \ }
    let jobid = termopen(s:cmd, s:callbacks)
endf
