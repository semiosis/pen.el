function! MovePercent(signedpercent, mode)
    if a:mode ==# 'v'
        normal! gv
    endif

    let step = max([abs(a:signedpercent) * winheight(0) / 100, 1])
    if a:signedpercent < 0
        for i in range(1,step)
            exec "normal! \<C-e>"
        endfor
    elseif a:signedpercent > 0
        for i in range(1,step)
            exec "normal! \<C-y>"
        endfor
    endif
endfunction

