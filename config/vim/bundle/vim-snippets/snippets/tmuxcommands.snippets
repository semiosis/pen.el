	CURRENT_TMUX_WINDOW_WIDTH="$(tmux display-message -p -t \$TMUX_PANE '#{window_width}')"
	CURRENT_TMUX_WINDOW_HEIGTH="$(tmux display-message -p -t \$TMUX_PANE '#{window_height}')"
	${1}
